\part{Our extensions to PRISM}
\frame{\partpage}

\frame{
\frametitle{Our extensions to PRISM}
\begin{itemize}
\item<1-> Extended the kulang specification language to accept bidirectional data flow problems 
\item<2-> Extended the core solver to efficiently solve a bidirectional data flow problem.
\end{itemize}
}

\frame{
\frametitle{Extensions to kulang}
We extended the specifications so that they can accept
\begin{itemize}
\item<1-> Forward and backward flow functions and meet functions
\item<2-> Forward and backward lattice types
\item<3-> Forward and backward boundry values and top values
\end{itemize}
}

\frame{
\frametitle{Extensions to core solver}
\onslide<1->
\psset{unit=1mm}
\psset{linewidth=.3mm}
\psset{framearc=.1}
\begin{pspicture}(25,-25)(38,64)
%\psframe(0,0)(38,64)
\putnode{n0}{origin}{52}{50}{\psframebox{Input program} }
\putnode{n1}{n0}{0}{-35}{\psframebox{Generated component} }
\putnode{n2}{n0}{55}{0}{\psframebox{Solver} }
\putnode{n3}{n0}{40}{-35}{\psframebox{DFResults} }
\putnode{n4}{n0}{85}{0}{\psframebox{Output reports} }
\ncline{->}{n0}{n2}
